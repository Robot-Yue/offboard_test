<launch>

	<arg name="fcu_url" default="/dev/ttyTHS0:500000" />
	<arg name="gcs_url" default="" />
	<arg name="tgt_system" default="1" />
	<arg name="tgt_component" default="1" />
	<arg name="log_output" default="screen" />
	<arg name="fcu_protocol" default="v2.0" />
	<arg name="respawn_mavros" default="false" />
	<arg name="set_lidar_frame_id" default="base_scan"/>


	<include file="$(find common_base)/launch/mavros_node.launch">
		<arg name="pluginlists_yaml" value="$(find common_base)/launch/mavros_apm_pluginlists.yaml" />
		<arg name="config_yaml" value="$(find common_base)/launch/mavros_apm_config.yaml" />

		<arg name="fcu_url" value="$(arg fcu_url)" />
		<arg name="gcs_url" value="$(arg gcs_url)" />
		<arg name="tgt_system" value="$(arg tgt_system)" />
		<arg name="tgt_component" value="$(arg tgt_component)" />
		<arg name="log_output" value="$(arg log_output)" />
		<arg name="fcu_protocol" value="$(arg fcu_protocol)" />
		<arg name="respawn_mavros" value="$(arg respawn_mavros)" />
		

		<!--remap mavros's topics to Ros's topics-->
		<!-- <remap from="/mavros/setpoint_position/local" to="/move_base_simple/goal" /> -->

	</include>

	<!-- 处理协方差 -->
	<include file="$(find ros_offboard_pkg)/launch/mavros_offboard_filter_msg.launch">
	</include>

</launch>
